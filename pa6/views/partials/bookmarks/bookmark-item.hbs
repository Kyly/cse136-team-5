<li class="list-item-style">
    <div class="bookmark-item split-list" >
        <div>
            <a class="bookmark-link" href="{{url}}" target="_blank">
                <span class=" icon-bookmark bookmark-icons hand"></span> HI {{ name }}
            </a>
        </div>
       <div>
           <a title="Favorite" class="action-button" href="/bookmarks/favorite?id={{id}}&fav={{favorite}}">
               {{#if favorite}}
                    <span class="fa icon-star edit-favorite-icons hand"></span>
               {{else}}
                  <span class="fa icon-star-empty edit-favorite-icons hand" ></span>
               {{/if}}</a>
           
           <span class="icon-edit edit-favorite-icons hand" onclick="app.bookmarkEdit.show(event, {{id}})"></span>
       </div>
    </div>
</li>
