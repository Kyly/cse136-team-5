{
  "name": "init",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "bower install --verbose && grunt",
    "start": "node ./bin/www",
    "seed:users": "./node_modules/.bin/sequelize db:seed --config database/config/config.json --seeders-path database/seeders --models-path database/models --seed user-seed.js",
    "seed:bookmarks": "./node_modules/.bin/sequelize db:seed --config database/config/config.json --seeders-path database/seeders --models-path database/models --seed bookmarks-seed.js",
    "seed:all": "npm run-script seed:users && npm run-script seed:bookmarks",
    "seed:undo": "./node_modules/.bin/sequelize db:seed:undo:all --config database/config/config.json --seeders-path database/seeders --models-path database/models --migrations-path database/migrations",
    "migration:all": "./node_modules/.bin/sequelize db:migrate --config database/config/config.json --migrations-path database/migrations",
    "migration:seed": "npm run-script migration:all && npm run-script seed:all",
    "migration:undo": "npm run-script seed:undo && npm run-script migration:all"
  },
  "dependencies": {
    "bcrypt": "^0.8.6",
    "body-parser": "~1.12.0",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.4",
    "csvtojson": "^0.5.12",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "express-handlebars": "^3.0.0",
    "express-history-api-fallback": "^2.0.0",
    "express-session": "^1.13.0",
    "handlebars": "^4.0.5",
    "hbs": "~2.9.0",
    "ip": "^1.1.3",
    "json2csv": "^3.4.1",
    "lodash": "^4.12.0",
    "morgan": "~1.5.1",
    "multer": "^1.1.0",
    "mysql": "^2.10.2",
    "password-reset": "^0.1.4",
    "sequelize": "^3.23.2",
    "sequelize-isunique-validator": "^1.1.1",
    "serve-favicon": "~2.2.0",
    "sql-query": "^0.1.26",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-handlebars": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-express-server": "^0.5.3",
    "grunt-w3c-html-validation": "^0.1.8",
    "sequelize-cli": "^2.4.0"
  }
}
