<bm-upload-file-dialog class="dialog">
    <div class="dialog-header">
        <div class="dialog-title">Upload</div>
        <bm-dialog-close><span onclick="app.remove(event, this, 'bm-upload-file-dialog')" class=" icon-cancel"></span></bm-dialog-close>
    </div>
    <bm-dialog-body class="drag-and-drop-container dropzone dz-clickable">
        <div class="drag-and-drop-inner dz-message">
            <div class="text-center">
                <span class="icon-upload-cloud-1"></span>
                <p>Drag bookmarks file here (*.csv)</p>
            </div>
        </div>
    </bm-dialog-body>
</bm-upload-file-dialog>